<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.carts.css">
    <title>Categorias</title>
</head>
<body>
    <header>
        <nav>
            <ul>
                <a href="/">Inicio</a>
                <a href="/productos">Productos</a>
                <a href="/usuarios">Usuarios</a>
                <a href="/carts">Carts</a>
            </ul>
        </nav>
    </header>
    <section>
        <h1>Categorias</h1>
        <a href="/categorias3/Crear"><h2>Crear Nueva Categoria</h2></a>
        <ul>
            <% categorias.forEach(categoria => { %>
                <li><a href="/categorias3/<%= categoria.id %>"><%= categoria.nombre %></a></li>
            <% }); %>
        </ul>
    </section>
</body>
</html>